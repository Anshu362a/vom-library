<h2>Visitor Logs</h2>
<table>
  <thead>
    <tr>
      <th>#</th>
      <th>Device</th>
      <th>IP</th>
      <th>Location</th>
      <th>Coordinates</th>
      <th>Visited At</th>
    </tr>
  </thead>
  <tbody>
    <% if (visits && visits.length > 0) { %>
      <% visits.forEach((v, i) => { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= v.device || "Unknown" %></td>
          <td><%= v.ip || "N/A" %></td>
          <td><%= v.location || "Unknown" %></td>
          <td>
            <% if (v.coordinates && v.coordinates.length === 2) { %>
              <%= v.coordinates[1] %>, <%= v.coordinates[0] %>
              <br />
              <a 
                href="https://www.google.com/maps?q=<%= v.coordinates[1] %>,<%= v.coordinates[0] %>" 
                target="_blank" 
                style="color: #FA812F; text-decoration: underline;"
              >
                View on Map
              </a>
            <% } else { %>
              Not available
            <% } %>
          </td>
          <td><%= moment(v.visitedAt).tz("Asia/Kolkata").format("DD MMM YYYY, hh:mm A") %></td>
        </tr>
      <% }) %>
    <% } else { %>
      <tr>
        <td colspan="6">No visitor data found.</td>
      </tr>
    <% } %>
  </tbody>
</table>
